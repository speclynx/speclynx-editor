    "panelTitle.inactiveForeground": "#5a7d9d",

    "terminal.foreground": "#d4d8e0",
    "terminal.background": "#0a1828",

    "input.background": "#102030",
    "input.foreground": "#d4d8e0",
    "input.border": "#1d3d5d",
    "input.placeholderForeground": "#4d6d8d",
    "inputOption.activeBorder": "#449ccf",
    "focusBorder": "#449ccf",

    "button.background": "#449ccf",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#3588b8",

    "dropdown.background": "#102030",
    "dropdown.border": "#1d3d5d",
    "dropdown.foreground": "#d4d8e0",

    "badge.background": "#449ccf",
    "badge.foreground": "#ffffff",

    "scrollbar.shadow": "#00000060",
    "scrollbarSlider.background": "#1d3d5d40",
    "scrollbarSlider.hoverBackground": "#1d4d7a60",
    "scrollbarSlider.activeBackground": "#449ccf80",

    "breadcrumb.foreground": "#5a7d9d",
