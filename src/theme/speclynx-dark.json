    "editor.findMatchBackground": "#449ccf50",
    "editor.findMatchHighlightBackground": "#449ccf30",
    "editor.wordHighlightBackground": "#1d4d7a40",
    "editor.wordHighlightStrongBackground": "#1d4d7a60",

    "editorCursor.foreground": "#7ED8F5",
    "editorLineNumber.foreground": "#3a5575",
    "editorLineNumber.activeForeground": "#7ED8F5",
    "editorIndentGuide.background1": "#1a3555",
    "editorIndentGuide.activeBackground1": "#2a5585",
