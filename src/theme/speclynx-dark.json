    "editorSuggestWidget.background": "#102030",
    "editorSuggestWidget.border": "#1d3d5d",
    "editorSuggestWidget.selectedBackground": "#1d4d7a",
    "editorSuggestWidget.selectedForeground": "#ffffff",
    "editorSuggestWidget.focusHighlightForeground": "#7ED8F5",
    "editorHoverWidget.background": "#102030",
    "editorHoverWidget.border": "#1d3d5d",

    "activityBar.background": "#081422",
    "activityBar.foreground": "#7ED8F5",
    "activityBar.inactiveForeground": "#3d5d7d",
    "activityBar.border": "#0c1e30",
    "activityBarBadge.background": "#449ccf",
    "activityBarBadge.foreground": "#ffffff",

    "sideBar.background": "#0a1828",
    "sideBar.foreground": "#a0b5c8",
    "sideBar.border": "#0c1e30",
    "sideBarTitle.foreground": "#7ED8F5",
    "sideBarSectionHeader.background": "#0e2238",
    "sideBarSectionHeader.foreground": "#d4d8e0",

    "list.activeSelectionBackground": "#1d4d7a",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#112d4d",
    "list.hoverBackground": "#112840",
    "list.highlightForeground": "#7ED8F5",

    "statusBar.background": "#163657",
    "statusBar.foreground": "#7ED8F5",
    "statusBar.border": "#1d4570",
    "statusBar.debuggingBackground": "#449ccf",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#0c1e30",
    "statusBarItem.hoverBackground": "#1d4570",
    "statusBarItem.remoteBackground": "#449ccf",
    "statusBarItem.remoteForeground": "#ffffff",

    "titleBar.activeBackground": "#163657",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#163657",
    "titleBar.inactiveForeground": "#7ED8F5",
    "titleBar.border": "#0c1e30",

    "tab.activeBackground": "#0c1e30",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#081422",
    "tab.inactiveForeground": "#5a7d9d",
    "tab.border": "#081422",
    "tab.activeBorderTop": "#449ccf",
    "tab.unfocusedActiveBorderTop": "#1d3d5d",

    "editorGroupHeader.tabsBackground": "#081422",
    "editorGroupHeader.tabsBorder": "#0c1e30",

    "panel.background": "#0a1828",
    "panel.border": "#1d3555",
    "panelTitle.activeBorder": "#449ccf",
