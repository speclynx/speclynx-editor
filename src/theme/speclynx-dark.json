    "minimap.background": "#0a1828",

    "notificationCenter.border": "#1d3d5d",
    "notifications.background": "#102030",
    "notifications.border": "#1d3d5d",
    "notificationsErrorIcon.foreground": "#f44747",
    "notificationsWarningIcon.foreground": "#cca700",
    "notificationsInfoIcon.foreground": "#449ccf",

    "peekView.border": "#449ccf",
    "peekViewEditor.background": "#0c1e30",
    "peekViewResult.background": "#0a1828",
    "peekViewTitle.background": "#102030",

    "gitDecoration.modifiedResourceForeground": "#449ccf",
    "gitDecoration.addedResourceForeground": "#10B981",
    "gitDecoration.deletedResourceForeground": "#f44747",
    "gitDecoration.untrackedResourceForeground": "#10B981",

    "debugToolBar.background": "#102030",
    "progressBar.background": "#449ccf",

    "textLink.foreground": "#7ED8F5",
    "textLink.activeForeground": "#449ccf",
    "descriptionForeground": "#7d9db5",
    "icon.foreground": "#7ED8F5",

    "welcomePage.background": "#0c1e30",
    "walkThrough.embeddedEditorBackground": "#0a1828"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#3d5d7d", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#10B981" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#7ED8F5" }
    },
    {
      "name": "Booleans",
      "scope": ["constant.language.boolean"],
      "settings": { "foreground": "#7ED8F5" }
    },
    {
      "name": "Null",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": { "foreground": "#7ED8F5", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "storage.type"],
      "settings": { "foreground": "#449ccf" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#d4d8e0" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#3588b8" }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.type", "entity.name.class", "support.type"],
      "settings": { "foreground": "#7ED8F5" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#d4d8e0" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#a0b5c8", "fontStyle": "italic" }
    },
    {
      "name": "Properties / Keys",
      "scope": ["support.type.property-name", "entity.name.tag.yaml", "entity.other.attribute-name"],
      "settings": { "foreground": "#449ccf" }
    },
    {
      "name": "YAML Tags & Anchors",
      "scope": ["entity.name.tag.yaml", "keyword.control.flow.block-scalar.literal.yaml", "keyword.control.flow.block-scalar.folded.yaml"],
      "settings": { "foreground": "#449ccf", "fontStyle": "bold" }
    },
    {
      "name": "YAML Punctuation",
      "scope": ["punctuation.definition.block.sequence.item.yaml", "punctuation.separator.key-value.mapping.yaml"],
      "settings": { "foreground": "#5d7d9d" }
    },
    {
      "name": "JSON Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#449ccf" }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#7ED8F5", "fontStyle": "bold" }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": { "foreground": "#7ED8F5" }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#449ccf" }
    },
    {
      "name": "Attribute Values",
      "scope": ["string.quoted.double.html", "string.quoted.single.html"],
      "settings": { "foreground": "#10B981" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#f44747" }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#7ED8F5" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": { "foreground": "#5d7d9d" }
    },
    {
      "name": "Reference / $ref",
      "scope": ["string.other.link", "markup.underline"],
      "settings": { "foreground": "#ff9966" }
    }
  ]
}
