    "menus": {
      "editor/context": [
        {
          "submenu": "openapiToolkit.editor.context",
          "group": "z_commands"
        }
      ],
      "explorer/context": [
        {
          "submenu": "openapiToolkit.explorer.context",
          "group": "2_workspace"
        }
      ],
      "commandPalette": [
        {
          "command": "openapiToolkit.preview",
          "when": "editorLangId == apidom"
        },
        {
          "command": "openapiToolkit.dereferenceDocument",
          "when": "editorLangId == apidom"
        },
        {
          "command": "openapiToolkit.convertJsonToYaml",
          "when": "editorLangId == apidom && openapiToolkit.isApiDOMJSON"
        },
        {
          "command": "openapiToolkit.convertYamlToJson",
          "when": "editorLangId == apidom && openapiToolkit.isApiDOMYAML"
        },
        {
          "command": "openapiToolkit.pickSpectralRulesFile",
          "when": "editorLangId == apidom"
        },
        {
          "command": "openapiToolkit.pickRulesFile",
          "when": "editorLangId == apidom"
        },
        {
          "command": "openapiToolkit.reloadRules",
          "when": "editorLangId == apidom"
        }
      ],
      "openapiToolkit.explorer.context": [
        {
          "command": "openapiToolkit.preview",
          "when": "resourceLangId == apidom",
          "group": "2_workspace@1"
        },
        {
          "command": "openapiToolkit.dereferenceDocument",
          "when": "resourceLangId == apidom",
          "group": "2_workspace@2"
        },
        {
          "command": "openapiToolkit.convertJsonToYaml",
          "when": "resourceLangId == apidom && openapiToolkit.isApiDOMJSON",
          "group": "2_workspace@3"
        },
        {
          "command": "openapiToolkit.convertYamlToJson",
          "when": "resourceLangId == apidom && openapiToolkit.isApiDOMYAML",
          "group": "2_workspace@4"
        },
        {
          "command": "openapiToolkit.pickSpectralRulesFile",
          "when": "resourceLangId == apidom",
          "group": "2_workspace@5"
        },
        {
          "command": "openapiToolkit.pickRulesFile",
          "when": "resourceLangId == apidom",
          "group": "2_workspace@6"
        },
        {
          "command": "openapiToolkit.reloadRules",
          "when": "resourceLangId == apidom",
          "group": "2_workspace@7"
        }
      ],
      "openapiToolkit.editor.context": [
        {
          "command": "openapiToolkit.preview",
          "when": "editorLangId == apidom",
          "group": "z_commands@1"
        },
        {
          "command": "openapiToolkit.dereferenceDocument",
          "when": "editorLangId == apidom",
          "group": "z_commands@2"
        },
        {
          "command": "openapiToolkit.convertJsonToYaml",
          "when": "editorLangId == apidom && openapiToolkit.isApiDOMJSON",
          "group": "z_commands@3"
        },
        {
          "command": "openapiToolkit.convertYamlToJson",
          "when": "editorLangId == apidom && openapiToolkit.isApiDOMYAML",
          "group": "z_commands@4"
        },
        {
          "command": "openapiToolkit.pickSpectralRulesFile",
          "when": "editorLangId == apidom",
          "group": "z_commands@5"
        },
        {
          "command": "openapiToolkit.pickRulesFile",
          "when": "editorLangId == apidom",
          "group": "z_commands@6"
        },
        {
          "command": "openapiToolkit.reloadRules",
          "when": "editorLangId == apidom",
          "group": "z_commands@7"
        }
      ]
