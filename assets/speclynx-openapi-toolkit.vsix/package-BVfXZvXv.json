{
  "name": "vscode-openapi-toolkit",
  "displayName": "OpenAPI Toolkit",
  "description": "OpenAPI editing, commands, validation, linting & live preview in VSCode",
  "version": "1.5.1",
  "icon": "./images/icon.png",
  "homepage": "https://speclynx.com",
  "publisher": "SpecLynx",
  "author": "SpecLynx",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Francesco Tumanischvili",
      "email": "frantuma@yahoo.com"
    },
    {
      "name": "VladimÃ­r Gorej",
      "email": "vladimir.gorej@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/speclynx/vscode-openapi-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/speclynx/vscode-openapi-toolkit/issues"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Visualization"
  ],
  "keywords": [
    "openapi",
    "swagger",
    "json",
    "yaml",
    "editor",
    "toolkit",
    "validate",
    "lint",
    "format",
    "convert",
    "preview",
    "api",
    "api description",
    "api specification",
    "oas",
    "oas2",
    "oas3"
  ],
  "engines": {
    "vscode": "^1.75.0",
    "node": ">=16.14.2"
  },
  "pricing": "Free",
  "sponsor": {
    "url": "https://github.com/speclynx/vscode-openapi-toolkit"
  },
  "main": "./client/dist/node/speclynxClientMain.js",
  "browser": "./client/dist/browser/speclynxClientMain.js",
  "scripts": {
    "vscode:package": "vsce package --out 'bin'",
    "build:dev": "npm --workspace=client run build:dev && tsc -b --verbose",
    "build:dev:watch": "npm --workspace=client run build:dev && tsc -b --verbose --watch",
    "build:prod": "npm run build:dev && npm run build:prod --workspaces --if-present",
    "build:prod:browser": "npm run build:dev && npm run build:prod:browser --workspaces --if-present",
    "build:prod:node": "npm run build:dev && npm run build:prod:node --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "test": "npm --workspace=server run test && sh ./scripts/e2e.sh",
    "test:web": "vscode-test-web --extensionDevelopmentPath=. --browser=chromium --verbose",
    "typescript:check-types": "tsc -b --verbose --noEmit"
  },
  "files": [
    "client/dist/node/speclynxClientMain.js",
    "client/dist/browser/speclynxClientMain.js",
    "client/dist/preview/webview",
    "images/icon.png",
    "server/dist/node/speclynxServerMain.js",
    "server/dist/browser/speclynxServerMain.js",
    "language-configuration.json",
    "LICENSE",
    "NOTICE",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "SUPPORT.md"
  ],
