    "configuration": [
      {
        "id": "speclynx.openapi.preview",
        "title": "Preview",
        "order": 0,
        "type": "object",
        "properties": {
          "speclynx.openapi.preview.renderer": {
            "scope": "resource",
            "type": "string",
            "enum": [
              "scalar",
              "swagger-ui"
            ],
            "default": "scalar",
            "description": "Select which renderer to use for API documentation preview."
          }
        }
      },
      {
        "id": "speclynx.openapi.validation.jsonSchema",
        "title": "JSON Schema Validation",
        "order": 1,
        "type": "object",
        "properties": {
          "speclynx.openapi.validation.applyJsonSchemaValidation": {
            "scope": "resource",
            "type": "boolean",
            "default": true,
            "description": "Applies JSON Schema validation."
          },
          "speclynx.openapi.validation.betterErrorsForJsonSchema": {
            "scope": "resource",
            "type": "boolean",
            "default": true,
            "description": "Uses better error messages for JSON Schema validation."
          }
        }
      },
      {
        "id": "speclynx.openapi.validation.semantic",
        "title": "Semantic Validation",
        "order": 2,
        "type": "object",
        "properties": {
          "speclynx.openapi.validation.applySemanticValidation": {
            "scope": "resource",
            "type": "boolean",
            "default": false,
            "description": "Applies semantic validation."
          },
          "speclynx.openapi.validation.referenceValidation": {
            "scope": "resource",
            "type": "boolean",
            "default": true,
            "description": "Applies reference validation."
          },
          "speclynx.openapi.validation.semanticLinting": {
            "scope": "resource",
            "type": "boolean",
            "default": true,
            "description": "Applies semantic linting (Warning/Info level)."
          },
          "speclynx.openapi.validation.semanticRulesFile": {
            "scope": "resource",
            "type": "string",
            "description": "Location of the file to use for custom semantic validation/linting.\nDefaults to (.)validation.yaml/yml or (.)validation.json in the workspace root.\n\nWhen the extension runs in a browser-based VS Code instance (e.g. vscode.dev or a GitHub Codespace), only files that reside inside the current workspace can be referenced."
          },
          "speclynx.openapi.validation.semanticMaxNumberOfProblems": {
            "scope": "resource",
            "type": "number",
            "default": 100,
            "description": "Controls the maximum number of problems produced by the server."
          }
        }
      },
      {
        "id": "speclynx.openapi.validation.spectral",
        "title": "Spectral Validation",
        "order": 3,
        "type": "object",
        "properties": {
          "speclynx.openapi.validation.applySpectralValidation": {
            "scope": "resource",
            "type": "boolean",
            "default": false,
            "description": "Applies Spectral validation/linting."
          },
          "speclynx.openapi.validation.spectralRulesFile": {
            "scope": "resource",
            "type": "string",
            "description": "Location of the file to use for Spectral validation/linting.\nDefaults to (.)spectral.yaml/yml or (.)spectral.json in the workspace root.\n\nWhen the extension runs in a browser-based VS Code instance (e.g. vscode.dev or a GitHub Codespace), only files that reside inside the current workspace can be referenced."
          },
          "speclynx.openapi.validation.markFirstLineOnlyOfSpectralProblem": {
            "scope": "resource",
            "type": "boolean",
            "default": true,
            "description": "If true, mark only the first line of any Spectral violation."
          }
        }
      },
      {
        "id": "speclynx.openapi.logging",
        "title": "Logging",
        "order": 4,
        "type": "object",
        "properties": {
          "speclynx.openapi.logging.server": {
            "scope": "resource",
            "type": "string",
            "enum": [
              "off",
              "messages",
              "verbose"
            ],
            "default": "messages",
            "tags": [
              "logging"
            ],
            "description": "Traces the communication between VS Code and the language server."
          }
        }
      }
    ],
    "commands": [
      {
        "command": "openapiToolkit.preview",
        "title": "Show API Document preview",
        "category": "OpenAPI Toolkit"
